<?xml version="1.0" ?>
<robot name="2WD_rover_robotics" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find openrover_demo)/urdf/common_properties.xacro"/>
  <xacro:include filename="$(find openrover_demo)/urdf/2WD_Rover.gazebo.xacro"/>

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.12" rpy="0 0 0"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin xyz="-0.142 0 0.055" rpy="0 0 1.57"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/2WD_OpenRover.stl" scale="1 1 1"/>
      <!--
      <origin xyz="-0.142 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.42 0.25 0.12"/>
        -->
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="-0.142 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.42 0.25 0.12"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.142 0 0" rpy="0 0 0" />
      <mass value="7.5" />
      <inertia ixx="4.806e-2"
               ixy="0.0" iyy="1.193e-1"
               ixz="0.0" iyz="0.0" izz="1.493e-1" />
    </inertial>
  </link>



  <joint name="wheel_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <origin xyz="0 0.179 0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="-1.57 0 0"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/left_wheel.stl" scale="1 1 1"/>
      <!--
      <origin xyz="0 0 0" rpy="0 0 3.14"/>
      <geometry>
        <cylinder length="0.07" radius="0.12"/>
        -->
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 3.14"/>
      <geometry>
        <cylinder length="0.07" radius="0.12"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 3.14" />
      <mass value="1.525" />
      <inertia ixx="6.723e-3"
               ixy="0.0" iyy="6.723e-3"
               ixz="0.0" iyz="0.0" izz="1.220e-2" />
    </inertial>
  </link>



  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0 -0.179 0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="wheel_right_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/right_wheel.stl" scale="1 1 1"/>
      <!--
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.12"/>
        -->
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.12"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.525" />
      <inertia ixx="6.723e-3"
               ixy="0.0" iyy="6.723e-3"
               ixz="0.0" iyz="0.0" izz="1.220e-2" />
    </inertial>
  </link>



  <joint name="sheetmetal_joint" type="fixed">
    <parent link="base_link"/>
    <child link="sheetmetal_link"/>
    <origin xyz="-0.35 0 0" rpy="0 0 0"/>
  </joint>

  <link name="sheetmetal_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 -1.57"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/sheetmetal_part.stl" scale="1 1 1"/>
      <!--
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.065 0.4 0.008"/>
        -->
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="-0.007 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.065 0.4 0.008"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.6" />
      <inertia ixx="8.020e-3"
               ixy="0.0" iyy="2.313e-4"
               ixz="0.0" iyz="0.0" izz="8.211e-3" />
    </inertial>
  </link>



  <joint name="plate_link_joint" type="fixed">
    <parent link="base_link"/>
    <child link="plate_link"/>
    <origin xyz="-0.1365 0 0.127" rpy="0 0 0"/>
  </joint>

  <link name="plate_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.381 0.254 0.005"/>
      </geometry>
      <material name="acrylic"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.65" />
      <inertia ixx="3.496e-3"
               ixy="0.0" iyy="7.864e-3"
               ixz="0.0" iyz="0.0" izz="1.136e-2" />
    </inertial>
  </link>



  <joint name="imu_link_joint" type="fixed">
    <parent link="plate_link"/>
    <child link="imu_link"/>
    <origin xyz="0.1365 0 0.0026" rpy="0 0 0"/>
  </joint>

  <link name="imu_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.002"/>
      </geometry>
      <material name="blue"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.04" />
      <inertia ixx="3.467e-7"
               ixy="0.0" iyy="3.467e-7"
               ixz="0.0" iyz="0.0" izz="6.667e-7" />
    </inertial>
  </link>



  <joint name="cpu_link_joint" type="fixed">
    <parent link="plate_link"/>
    <child link="cpu_link"/>
    <origin xyz="-0.0032 0 0.0275" rpy="0 0 0"/>
  </joint>

  <link name="cpu_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.111 0.117 0.05"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.64" />
      <inertia ixx="8.634e-4"
               ixy="0.0" iyy="7.905e-4"
               ixz="0.0" iyz="0.0" izz="1.387e-3" />
    </inertial>
  </link>



  <joint name="lidar_link_joint" type="fixed">
    <parent link="cpu_link"/>
    <child link="lidar_link"/>
    <origin xyz="0.10795 0 0.0635" rpy="0 0 3.14"/>
  </joint>

  <link name="lidar_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.0476" radius="0.0381"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.220" />
      <inertia ixx="1.214e-4"
               ixy="0.0" iyy="1.214e-4"
               ixz="0.0" iyz="0.0" izz="1.597e-4" />
    </inertial>
  </link>



  <joint name="caster_back_left_joint" type="continuous">
    <parent link="sheetmetal_link"/>
    <child link="caster_back_left_link"/>
    <origin xyz="-0.01 0.17 -0.01" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.1" friction="0.01"/>
  </joint>

  <link name="caster_back_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/left_caster_wheel.stl" scale="1 1 1"/>
      <!--
      <origin xyz="-0.028 0 -0.065" rpy="1.57 0 3.14"/>
      <geometry>
        <cylinder length="0.035" radius="0.048"/>
        -->
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="-0.035 0 -0.065" rpy="1.57 0 3.14"/>
      <geometry>
        <cylinder length="0.0035" radius="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.005 0 -0.03" />
      <mass value="0.41" />
      <inertia ixx="3.841e-4"
               ixy="0.0" iyy="4.592e-4"
               ixz="0.0" iyz="0.0" izz="2.135e-4" />
    </inertial>
  </link>



  <joint name="caster_back_right_joint" type="continuous">
    <parent link="sheetmetal_link"/>
    <child link="caster_back_right_link"/>
    <origin xyz="-0.01 -0.17 -0.01" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.1" friction="0.01"/>
  </joint>

  <link name="caster_back_right_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
      <geometry>
        <mesh filename="package://openrover_demo/meshes/right_caster_wheel.stl" scale="1 1 1"/>
      <!--
      <origin xyz="-0.028 0 -0.065" rpy="1.57 0 3.14"/>
      <geometry>
        <cylinder length="0.035" radius="0.048"/>
        -->
      </geometry>
      <material name="grey"/>
    </visual>

    <collision>
      <origin xyz="-0.035 0 -0.065" rpy="1.57 0 3.14"/>
      <geometry>
        <cylinder length="0.0035" radius="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="-0.005 0 -0.03" />
      <mass value="0.41" />
      <inertia ixx="3.841e-4"
               ixy="0.0" iyy="4.592e-4"
               ixz="0.0" iyz="0.0" izz="2.135e-4" />
    </inertial>
  </link>



</robot>
